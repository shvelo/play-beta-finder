{
  "manifest_version": 2,
  "name": "Play Open Beta Finder",
  "description": "Finds if the Google Play app has open beta testing",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://play.google.com/store/apps/*"
      ],
      "js": [
        "eventPage.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "https://play.google.com/*"
  ]
}